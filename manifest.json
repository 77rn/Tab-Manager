{
    "manifest_version": 3,
    "name": "Tab Manager- Group, Organize, Search, and Close Tabs",
    "version": "1.0",
    "description": "Manage and organize your browser tabs effortlessly.",
    "permissions": ["tabs", "storage","tabGroups"],
    "host_permissions": ["<all_urls>"],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
  }
  